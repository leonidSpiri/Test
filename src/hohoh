public class T3 {

	public static void main(String[] args) {
System.out.println(F(100));

	}
	
	static int F(int n) {
		if (n<=3) return n;
		if (n >3 && (n %3 == 0)) return n * n * n + F(n - 1);
		if (n >3 && (n %3 == 1)) return 4 + F(n / 3);
		if (n >3 && (n %3 == 2)) return n * n + F(n - 2);
		return 0;
	}

}
